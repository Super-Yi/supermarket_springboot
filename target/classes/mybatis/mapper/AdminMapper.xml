<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.superyi.supermarket_springboot.model.dao.AdminDao">
    <!--查询用户-->
    <select id="getAdmin" resultType="com.superyi.supermarket_springboot.model.entity.User">
    select * from sm_user where user_code=#{userCode} and user_password=#{userPassword}
    </select>
</mapper>